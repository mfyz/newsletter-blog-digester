{
  "incremental": true,
  "json": {
    "indentWidth": 2
  },
  "markdown": {
    "lineWidth": 80
  },
  "includes": [
    "*.{json,md}"
  ],
  "excludes": [
    "node_modules",
    "data.db",
    "*.log",
    "coverage"
  ],
  "plugins": [
    "https://plugins.dprint.dev/json-0.19.2.wasm",
    "https://plugins.dprint.dev/markdown-0.16.3.wasm"
  ]
}
